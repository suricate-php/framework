{
  "name": "suricate-php/framework",
  "description": "Suricate PHP framework",
  "keywords": [
    "framework",
    "suricate"
  ],
  "authors": [
    {
      "name": "Mathieu LESNIAK",
      "email": "mathieu@lesniak.fr"
    }
